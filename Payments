  <span class="token operator">&lt;</span>input type<span class="token operator">=</span><span class="token string">"hidden"</span> name<span class="token operator">=</span><span class="token string">"MD"</span> value<span class="token operator">=</span><span class="token string">"{MD}"</span> <span class="token operator">></span>
  <span class="token operator">&lt;</span>input type<span class="token operator">=</span><span class="token string">"hidden"</span> name<span class="token operator">=</span><span class="token string">"PaReq"</span> value<span class="token operator">=</span><span class="token string">"{PaReq}"</span> <span class="token operator">></span>
