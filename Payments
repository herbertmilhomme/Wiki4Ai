<span class="token punctuation">}</span>
sortedParameters<span class="token punctuation">.</span><span class="token function">put</span><span class="token punctuation">(</span>PASSWORD_KEY<span class="token punctuation">,</span> PASSWORD_VALUE<span class="token punctuation">)</span><span class="token punctuation">;</span> final String paramString <span class="token operator">=</span>
Joiner<span class="token punctuation">.</span><span class="token function">on</span><span class="token punctuation">(</span><span class="token string">""</span><span class="token punctuation">)</span><span class="token punctuation">.</span><span class="token function">skipNulls</span><span class="token punctuation">(</span><span class="token punctuation">)</span><span class="token punctuation">.</span><span class="token function">join</span><span class="token punctuation">(</span>sortedParameters<span class="token punctuation">.</span><span class="token function">values</span><span class="token punctuation">(</span><span class="token punctuation">)</span><span class="token punctuation">)</span><span class="token punctuation">;</span> <span class="token keyword">return</span>
