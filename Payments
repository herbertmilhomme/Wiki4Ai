                    <span class="token punctuation">{</span>
                        <span class="token string-property property">"Number"</span><span class="token operator">:</span> <span class="token string">"123/43"</span><span class="token punctuation">,</span>
                        <span class="token string-property property">"Date"</span><span class="token operator">:</span> <span class="token string">"21.11.2020"</span><span class="token punctuation">,</span>
